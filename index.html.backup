<script>
// Simple scroll handler using CSS variables
window.addEventListener("scroll", function() {
    const root = document.documentElement;
    if (window.scrollY > 50) {
        root.style.setProperty("--navbar-shadow", "0 2px 8px rgba(0,0,0,0.1)");
    } else {
        root.style.setProperty("--navbar-shadow", "none");
    }
});
</script>
<script src="linkedin-feed.js"></script>
<script>
// Remove any automatically injected "Open on LinkedIn" buttons while preserving "Follow on LinkedIn"
(function () {
    function removeOpenButtons(root) {
        var container = root || document;
        var links = container.querySelectorAll('.linkedin-placeholder a, .linkedin-embed a');
        links.forEach(function(link){
            var text = (link.textContent || '').trim().toLowerCase();
            if (text === 'open on linkedin') {
                link.remove();
            }
        });
    }
    // Initial pass after DOM ready
    if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', function(){ removeOpenButtons(); });
    } else {
        removeOpenButtons();
    }
    // Observe dynamic injections into the LinkedIn card
    var placeholder = document.querySelector('.linkedin-placeholder');
    if (placeholder && 'MutationObserver' in window) {
        var observer = new MutationObserver(function(mutations){
            mutations.forEach(function(m){ removeOpenButtons(m.target); });
        });
        observer.observe(placeholder, { childList: true, subtree: true });
    }
})();
</script>
<script>
// Handle form submission
document.getElementById('contactForm').addEventListener('submit', function(e) {
    e.preventDefault();
    
    // Get form data
    const formData = new FormData(this);
    const email = formData.get('email');
    const name = formData.get('name');
    const message = formData.get('message');
    
    // Send to Formspree
    fetch('https://formspree.io/f/mblzgbyy', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify({
            email: email,
            name: name,
            message: message
        })
    })
    .then(response => {
        if (response.ok) {
            // Hide form content
            this.style.display = 'none';
            // Show success message
            const successMsg = document.getElementById('successMessage');
            if (successMsg) {
                successMsg.style.display = 'flex';
                // Scroll to success message
                successMsg.scrollIntoView({ behavior: 'smooth' });
            } else {
                console.error('Success message element not found');
            }
        } else {
            alert('There was an error submitting the form. Please try again.');
        }
    })
    .catch(error => {
        console.error('Error:', error);
        alert('There was an error submitting the form. Please try again.');
    });
});
</script></body>
